<?xml version="1.0" encoding="UTF-8" ?><project name="Deployment" default="usage">  <property name="PHING_PATH" value=".phing" />  <!-- include all property files needed by phing -->  <property file="${PHING_PATH}/db.properties" />  <property name="db_url" value="jdbc:mysql://${db_host}/${db_name}?createDatabaseIfNotExist=true" />  <import file="${PHING_PATH}/liquibase.xml"/>  <!-- default task -->  <target name="usage">    <phingcall target="db:usage" />  </target></project>