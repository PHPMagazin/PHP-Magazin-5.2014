<?xml version="1.0" encoding="UTF-8" ?><project name="Database Deployment" default="db:usage">  <!-- print usage for phing build -->  <target name="db:usage">    <echo> ... Usage ... </echo>  </target>  <!-- updates your Database with all changelogs which are added to database/chanelogs.xml -->  <target name="db:update">    <liquibase-update changelogFile="${liquibase_changelog}" jar="${liquibase_jar}" classpathref="${liquibase_classpath}" username="${db_user}" password="${db_password}" url="${db_url}"/>  </target>  <!-- release the given Version and update your Database with the contained changelogs -->  <target name="db:release">    <input propertyName="sTag">${line.separator}Please Enter the tagname:</input>    <liquibase-update changelogFile="${liquibase_changelog_release}" jar="${liquibase_jar}" classpathref="${liquibase_classpath}" username="${db_user}" password="${db_password}" url="${db_url}"/>    <liquibase-tag tag="${sTag}" changelogFile="${liquibase_changelog_release}" jar="${liquibase_jar}" classpathref="${liquibase_classpath}" username="${db_user}" password="${db_password}" url="${db_url}" />  </target></project>